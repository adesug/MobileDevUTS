<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ade.sug.utsmobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UTSMOBILE">
        <activity android:name="ade.sug.utsmobile.UpdateActivity"></activity>
        <activity android:name="ade.sug.utsmobile.DetailActivity" />
        <activity android:name="ade.sug.utsmobile.CreateBarangActivity" />
        <activity android:name="ade.sug.utsmobile.BarangActivity" />
        <activity android:name="ade.sug.utsmobile.DashboardActivity" />
        <activity android:name="ade.sug.utsmobile.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ade.sug.utsmobile.MainActivity" />
    </application>

</manifest>